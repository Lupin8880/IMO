<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <h3>Send in the following content</h3>
    <%= form_for(@cinnabar) do |f| %>
      <%= f.hidden_field :content %>
      <p>Contentï¼š<%= @cinnabar.content %></p>
      <%= f.submit "Submit",class: 'btn btn-primary btn-block' %>
    <% end %>
    <%= form_for(@cinnabar, url: new_cinnabar_path, method: 'get') do |f| %>
      <%= f.hidden_field :content %>
      <%= f.submit "Back", name: 'back', class: 'btn btn-default btn-block' %>
    <% end %>
  </div>
</div>